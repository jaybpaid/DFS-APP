# OpenRouter Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL_LIST=openai/gpt-4,anthropic/claude-3-opus,google/gemini-pro

# Model Configuration
OC_DEFAULT_MODEL=openai/gpt-4
OC_TEMPERATURE=0.2
OC_MAX_TOKENS=2048

# Rate Limiting Configuration
OC_MAX_RETRIES=6
OC_BASE_DELAY_MS=500
OC_MAX_DELAY_MS=15000
OC_COOLDOWN_MS=60000

# Agent Configuration
AGENT_MAX_ITER=3

# Build Configuration
BUILD_OUTPUT=build.log

# System Message (optional)
OC_SYSTEM_MESSAGE=You are a helpful AI assistant specializing in code generation and software development.

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=opencode.log

# Cache Configuration
OC_CACHE_MODE=file
OC_CACHE_FILE=.cache/opencode.jsonl
OC_CACHE_SQLITE=.cache/opencode.db
OC_CACHE_TTL_MS=86400000
OC_CACHE_SALT=

# SSL Configuration
# Set to '0' to disable SSL certificate verification (NOT RECOMMENDED FOR PRODUCTION)
NODE_TLS_REJECT_UNAUTHORIZED=0

# Bedrock / AWS Bedrock integration
AWS_PROFILE=default
AWS_REGION=us-east-1
CLINE_API_PROVIDER=bedrock
CLINE_BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
AWS_ACCESS_KEY_ID=REDACTED
AWS_SECRET_ACCESS_KEY=REDACTED
